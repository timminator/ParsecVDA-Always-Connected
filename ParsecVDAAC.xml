<service>

  <!-- ID of the service. It should be unique across the Windows system-->
  <id>ParsecVDAAC</id>
  <name>ParsecVDA - Always Connected</name>
  <description>This service adds a Virtual Display to your system.</description>
  <!-- Path to the executable, which should be started -->
  <!-- CAUTION: Don't put arguments here. Use <arguments> instead. -->
  <executable>%BASE%\ParsecVDA - Always Connected.exe</executable>
</service>
